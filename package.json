{
  "author": "Evan Tahler <evantahler@gmail.com>",
  "name": "actionhero",
  "description": "The reusable, scalable, and quick node.js API server for stateless and stateful applications",
  "version": "24.0.4",
  "homepage": "http://www.actionherojs.com",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/actionhero/actionhero.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "keywords": [
    "api",
    "realtime",
    "socket",
    "http",
    "https",
    "web",
    "game",
    "cluster",
    "soa",
    "action",
    "task",
    "delay",
    "service",
    "tcp"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@types/glob": "^7.1.3",
    "@types/ioredis": "^4.17.7",
    "@types/node": "^14.14.6",
    "@types/uuid": "^8.3.0",
    "browser_fingerprint": "^2.0.2",
    "dot-prop": "^6.0.0",
    "etag": "^1.8.1",
    "formidable": "^1.2.2",
    "glob": "^7.1.6",
    "i18n": "^0.13.2",
    "ioredis": "^4.19.2",
    "is-running": "^2.1.0",
    "mime": "^2.4.6",
    "node-resque": "^8.0.3",
    "optimist": "^0.6.1",
    "primus": "^8.0.0",
    "qs": "^6.9.4",
    "uglify-js": "^3.11.5",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/puppeteer": "^5.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "puppeteer": "^5.5.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "ts-jest": "^26.4.3",
    "ts-node-dev": "^1.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  },
  "optionalDependencies": {},
  "bin": {
    "actionhero": "./dist/bin/actionhero.js"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/testPlugin",
      "<rootDir>/tmp"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "scripts": {
    "postinstall": "echo 'To generate a new actionhero project, run \"npx actionhero generate\"'",
    "test": "jest",
    "prepare": "npm run build && npm run docs",
    "pretest": "npm run lint && npm run build",
    "dev": "ts-node-dev --transpile-only --no-deps ./src/server",
    "debug": "tsc && ts-node-dev --transpile-only --no-deps --inspect -- ./src/server ",
    "start": "node ./dist/server.js",
    "build": "rm -rf dist && ./node_modules/.bin/tsc --sourceMap false --declaration",
    "docs": "typedoc --out docs --theme default --mode file",
    "lint": "prettier --check src __tests__",
    "pretty": "prettier --write src __tests__"
  }
}
