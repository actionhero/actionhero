{
  "author": "Evan Tahler <evantahler@gmail.com>",
  "name": "actionHero",
  "description": "actionHero is a node.js API framework for both tcp sockets, web sockets, and http clients.  The goal of actionHero are to create an easy-to-use toolkit for making reusable & scalable APIs",
  "version": "4.0.8",
  "homepage": "http://actionherojs.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/evantahler/actionHero.git"
  },
  "main": "api",
  "keywords": ["socket", "http", "https", "web sockets", "game", "cluster", "soa", "action", "task", "delay", "service", "api", "tcp"],
  "engines": {
    "node": ">=0.6.0"
  },
  "dependencies": {
    "browser_fingerprint":"0.0.x",
    "async":"0.1.x", 
    "request":"2.11.x",
    "validator":"0.4.x",
    "formidable":"1.0.x",
    "colors":"0.6.x",
    "data2xml":"0.8.x",
    "mime":"1.2.x",
    "redis":"0.8.x",
    "socket.io":"0.9.x",
    "optimist":"0.3.x"
  },
  "devDependencies": {
    "mocha":"1.6.x",
    "should":"1.2.x",
    "socket.io-client":"0.9.x",
    "docserv" : "0.0.x"
  },
  "bin": {
    "actionHero": "./bin/actionHero"
  },
  "main" : "actionHero.js",
  "scripts": {
    "postinstall": "node ./bin/actionHero intro",
    "help": "node ./bin/actionHero help",
    "test": "node ./node_modules/.bin/mocha --reporter spec",
    "start": "node ./bin/actionHero start"
  }
}
