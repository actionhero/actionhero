{
  "author": "Evan Tahler <evantahler@gmail.com>",
  "name": "actionhero",
  "description": "actionhero.js is a multi-transport API Server with integrated cluster capabilities and delayed tasks",
  "version": "16.0.0",
  "homepage": "http://www.actionherojs.com",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/evantahler/actionhero.git"
  },
  "main": "actionhero.js",
  "keywords": [
    "api",
    "realtime",
    "socket",
    "http",
    "https",
    "web",
    "game",
    "cluster",
    "soa",
    "action",
    "task",
    "delay",
    "service",
    "tcp"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "async": "^2.1.4",
    "browser_fingerprint": "^0.1.0",
    "etag": "^1.7.0",
    "fakeredis": "^1.0.0",
    "formidable": "^1.0.17",
    "i18n": "^0.8.3",
    "ioredis": "^2.4.1",
    "is-running": "^2.0.1",
    "mime": "^1.3.4",
    "node-resque": "^4.0.1",
    "optimist": "^0.6.1",
    "primus": "^6.0.2",
    "qs": "^6.0.1",
    "uglify-js": "^2.7.5",
    "uuid": "^3.0.0",
    "winston": "^2.0.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "mocha": "^3.2.0",
    "request": "^2.75.0",
    "standard": "^8.6.0"
  },
  "optionalDependencies": {},
  "bin": {
    "actionhero": "./bin/actionhero"
  },
  "standard": {
    "ignore": [
      "bin/templates",
      "client"
    ],
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "scripts": {
    "postinstall": "echo 'To generate a new actionhero project, run \"node ./node_modules/.bin/actionhero generate\"'",
    "help": "node ./bin/actionhero help",
    "test": "cross-env NODE_ENV=test mocha",
    "pretest": "standard",
    "start": "node ./bin/actionhero"
  }
}
