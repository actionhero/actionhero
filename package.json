{
  "author": "Evan Tahler <evantahler@gmail.com>",
  "name": "actionhero",
  "description": "actionhero.js is a multi-transport API Server with integrated cluster capabilities and delayed tasks",
  "version": "22.0.6",
  "homepage": "http://www.actionherojs.com",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/actionhero/actionhero.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "keywords": [
    "api",
    "realtime",
    "socket",
    "http",
    "https",
    "web",
    "game",
    "cluster",
    "soa",
    "action",
    "task",
    "delay",
    "service",
    "tcp"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@types/glob": "^7.1.1",
    "@types/ioredis": "^4.14.9",
    "@types/node": "^13.11.0",
    "@types/uuid": "^7.0.2",
    "browser_fingerprint": "^2.0.2",
    "dot-prop": "^5.2.0",
    "etag": "^1.8.1",
    "formidable": "^1.2.2",
    "glob": "^7.1.6",
    "i18n": "^0.8.6",
    "ioredis": "^4.16.1",
    "is-running": "^2.1.0",
    "mime": "^2.4.4",
    "node-resque": "^7.0.2",
    "optimist": "^0.6.1",
    "primus": "^7.3.4",
    "qs": "^6.9.3",
    "uglify-js": "^3.8.1",
    "uuid": "^7.0.3",
    "winston": "^3.2.1",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.2.7",
    "jest-environment-webdriver": "^0.2.0",
    "prettier": "^2.0.4",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "ts-jest": "^25.3.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "optionalDependencies": {},
  "bin": {
    "actionhero": "./dist/bin/actionhero.js"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/testPlugin"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "scripts": {
    "postinstall": "echo 'To generate a new actionhero project, run \"npx actionhero generate\"'",
    "test": "jest",
    "prepare": "npm run build && npm run docs",
    "pretest": "npm run lint && npm run build",
    "dev": "ts-node-dev --transpile-only --no-deps ./src/server",
    "debug": "ts-node-dev --transpile-only --no-deps --inspect -- ./src/server ",
    "start": "node ./dist/server.js",
    "build": "rm -rf dist && ./node_modules/.bin/tsc --sourceMap false --declaration",
    "docs": "typedoc --out docs --theme default --mode file",
    "lint": "prettier --check src __tests__",
    "pretty": "prettier --write src __tests__"
  }
}
