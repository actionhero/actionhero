{
  "author": "Evan Tahler <evantahler@gmail.com>",
  "name": "actionHero",
  "description": "actionHero.js is a multi-transport API Server with integrated cluster capabilites and delayed tasks",
  "version": "6.3.0",
  "homepage": "http://actionherojs.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/evantahler/actionHero.git"
  },
  "main": "actionHero.js",
  "keywords": [
    "socket",
    "http",
    "https",
    "web sockets",
    "game",
    "cluster",
    "soa",
    "action",
    "task",
    "delay",
    "service",
    "api",
    "tcp"
  ],
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "jake": "latest",
    "browser_fingerprint": "0.0.x",
    "async": "0.2.x",
    "validator": "1.1.x",
    "formidable": "1.0.x",
    "data2xml": "0.8.x",
    "mime": "1.2.x",
    "redis": "0.8.x",
    "optimist": "0.5.x",
    "winston": "0.7.2",
    "node-uuid": "1.4.x",
    "fakeredis": "0.1.x",
    "faye": "0.8.x",
    "faye-redis": "0.1.x"
  },
  "devDependencies": {
    "mocha": "1.9.x",
    "should": "1.2.x",
    "request": "2.21.x",
    "uglify-js": "2.4.x"
  },
  "bin": {
    "actionHero": "./bin/actionHero"
  },
  "scripts": {
    "postinstall": "node ./bin/actionHero intro",
    "help": "node ./bin/actionHero help",
    "test": "fakeredis=false node ./node_modules/.bin/mocha --reporter spec && fakeredis=true node ./node_modules/.bin/mocha --reporter spec",
    "start": "node ./bin/actionHero"
  },
  "prepublish": "./node_modules/.bin/uglifyjs ./public/javascript/actionHeroWebSocket.js --lint -o ./public/javascript/actionHeroWebSocket.min.js",
  "subdomain": "actionhero",
  "domains": ["demo.actionherojs.com", "actionherojs.com"]
}
