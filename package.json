{
  "author": "Evan Tahler <evantahler@gmail.com>",
  "name": "actionhero",
  "description": "actionhero.js is a multi-transport API Server with integrated cluster capabilities and delayed tasks",
  "version": "17.1.0",
  "homepage": "http://www.actionherojs.com",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/actionhero/actionhero.git"
  },
  "main": "index.js",
  "keywords": [
    "api",
    "realtime",
    "socket",
    "http",
    "https",
    "web",
    "game",
    "cluster",
    "soa",
    "action",
    "task",
    "delay",
    "service",
    "tcp"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "browser_fingerprint": "^1.0.1",
    "dot-prop": "^4.1.1",
    "etag": "^1.8.1",
    "formidable": "^1.0.17",
    "i18n": "^0.8.3",
    "ioredis": "^3.1.4",
    "is-running": "^2.0.1",
    "mime": "^2.0.2",
    "node-resque": "^5.0.0",
    "optimist": "^0.6.1",
    "primus": "^7.1.0",
    "qs": "^6.5.1",
    "uglify-js": "^3.1.2",
    "uuid": "^3.1.0",
    "winston": "^2.0.0",
    "ws": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "docdash-actionhero": "0.0.1",
    "dirty-chai": "^2.0.0",
    "jsdoc": "^3.5.5",
    "mocha": "^3.5.3",
    "request": "^2.82.0",
    "request-promise-native": "^1.0.5",
    "standard": "^10.0.3"
  },
  "optionalDependencies": {},
  "bin": {
    "actionhero": "./bin/actionhero"
  },
  "standard": {
    "ignore": [
      "bin/templates",
      "docs",
      "client"
    ],
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "scripts": {
    "postinstall": "echo 'To generate a new actionhero project, run \"node ./node_modules/.bin/actionhero generate\"'",
    "help": "node ./bin/actionhero help",
    "test": "cross-env NODE_ENV=test mocha",
    "pretest": "standard",
    "posttest": "npm run docs",
    "start": "node ./bin/actionhero",
    "docs": "jsdoc -c jsdoc-conf.json"
  }
}
